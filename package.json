{
  "name": "Chob",
  "version": "0.3",
  "description": "An universal app search tool for Linux",
  "main": "./build/src/cli.js",
  "bin": {
    "chob": "./build/src/cli.js"
  },
  "devDependencies": {
    "@types/jest": "^23.3.12",
    "@types/node": "latest",
    "jest": "~23.6.0",
    "pkg": "^4.4.0",
    "prettier": "1.15.3",
    "rimraf": "^2.6.3",
    "ts-jest": "~23.10.5",
    "tslib": "^1.9.3",
    "tslint": "latest",
    "tslint-config-prettier": "1.17.0",
    "tslint-microsoft-contrib": "latest",
    "tsutils": "latest",
    "typescript": "latest",
    "webpack": "^4.31.0",
    "webpack-cli": "^3.3.2"
  },
  "scripts": {
    "clean": "rimraf coverage build tmp",
    "build": "tsc -p tsconfig.release.json && webpack && pkg dist/chob.js",
    "build:watch": "tsc -w -p tsconfig.release.json",
    "lint": "tslint -t stylish --project \"tsconfig.json\"",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "postinstall": "yarn build"
  },
  "author": "Muhammed Kaplan <mmuhammedkaplan0@gmail.com>",
  "license": "Apache-2.0",
  "dependencies": {
    "colors": "^1.3.3",
    "commander": "^2.20.0",
    "opener": "^1.5.1",
    "prompts": "^2.0.4",
    "request": "^2.88.0"
  }
}
